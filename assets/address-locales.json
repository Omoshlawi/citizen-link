[
  {
    "code": "us-default",
    "country": "US",
    "regionName": "United States (Default)",
    "description": "Standard addressing convention for the United States with state, city, and ZIP code structure.",
    "formatSpec": {
      "displayTemplate": "{{address1}}, {{level2}}, {{level1}} {{postalCode}}, {{country}}",
      "levels": [
        {
          "level": "level1",
          "label": "State / Territory",
          "required": true,
          "aliases": ["state", "province"]
        },
        {
          "level": "level2",
          "label": "City",
          "required": true,
          "aliases": ["city", "town"]
        },
        {
          "level": "level3",
          "label": "County",
          "required": false
        }
      ],
      "postalCode": {
        "label": "ZIP Code",
        "required": true,
        "description": "US 5 digit ZIP with optional +4"
      },
      "metadata": {
        "validation": {
          "postalCode": "^[0-9]{5}(?:-[0-9]{4})?$"
        }
      }
    },
    "examples": [
      {
        "label": "White House",
        "address": {
          "address1": "1600 Pennsylvania Ave NW",
          "level2": "Washington",
          "level1": "District of Columbia",
          "postalCode": "20500",
          "country": "US"
        },
        "notes": "District of Columbia acts as both city and state."
      }
    ],
    "tags": ["default", "urban"]
  },
